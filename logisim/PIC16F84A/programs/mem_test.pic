# Basic varing of the memory
MOVLW 75     # write 75 to accu                                   W=75
MOVWF var0   # moving value in register 'var0'                    W=75
MOVLW 255    # write 255 to accu                                  W=255
MOVWF var1   # moving value in register 'var1'                    W=255
MOVF var1 1  # Check Z=1 since W=var1 from previous instruction   W=255
MOVF var0 0  # moving back value in 'var0' to accu(checking Z=0)  W=75
CLRF var0    # clear var0                                         W=75
CLRF var1    # clear var1                                         W=75
CLRW         # clear accu                                         W=0